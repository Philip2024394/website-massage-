{
  "name": "indastreet-massage-platform",
  "version": "2.0.0",
  "description": "Modular massage booking platform with separate apps for each user type",
  "type": "module",
  "packageManager": "pnpm@10.21.0",
  "scripts": {
    "cleanup": "pwsh -NoProfile -File cleanup-memory.ps1",
    "dev": "vite",
    "dev:open": "vite --open",
    "dev:main": "vite --open",
    "dev:stable": "vite --port 3000 --host",
    "dev:clean": "pwsh -NoProfile -File cleanup-memory.ps1 && vite",
    "stop:all": "pwsh -NoProfile -File cleanup-memory.ps1",
    "build:production": "cross-env NODE_ENV=production ROLLUP_NO_NATIVE=1 ROLLUP_NO_WASM=1 vite build",
    "serve:production": "npx serve dist -s -p 3000",
    "dev:therapist": "cd app/apps/therapist-dashboard && pnpm run dev",
    "dev:admin": "cd app/apps/admin-dashboard && pnpm run dev",
    "dev:place": "cd app/apps/place-dashboard && pnpm run dev",
    "dev:facial": "cd app/apps/facial-dashboard && pnpm run dev",
    "dev:auth": "cd app/apps/auth-app && pnpm run dev",
    "dev:all": "concurrently \"pnpm run dev:main\" \"pnpm run dev:place\" \"pnpm run dev:therapist\" \"pnpm run dev:facial\" \"pnpm run dev:admin\"",
    "dev:packages": "concurrently \"cd app/packages/auth && pnpm run dev\" \"cd app/packages/ui && pnpm run dev\" \"cd app/packages/config && pnpm run dev\"",
    "start:all": "pnpm run dev:all",
    "health-check": "node scripts/health-check.js",
    "open:main": "pwsh -NoProfile -Command Start-Process http://localhost:3000/",
    "open:therapist": "pwsh -NoProfile -Command Start-Process http://localhost:3005/",
    "open:admin": "pwsh -NoProfile -Command Start-Process http://localhost:3007/",
    "open:place": "pwsh -NoProfile -Command Start-Process http://localhost:3002/",
    "open:facial": "pwsh -NoProfile -Command Start-Process http://localhost:3006/",
    "build": "cross-env ROLLUP_NO_NATIVE=1 ROLLUP_NO_WASM=1 vite build",
    "build:netlify": "cross-env ROLLUP_NO_NATIVE=1 ROLLUP_NO_WASM=1 vite build && cd apps/therapist-dashboard && pnpm run build && cd ../.. && node scripts/merge-therapist-dist.js",
    "build:npm": "npm run type-check && cross-env ROLLUP_NO_NATIVE=1 ROLLUP_NO_WASM=1 vite build",
    "build:local": "cross-env ROLLUP_NO_NATIVE=1 tsc && vite build",
    "build:vite": "tsc && vite build --config vite.config.simple.ts",
    "build:simple": "tsc && vite build --config vite.config.simple.ts",
    "bs": "cross-env ROLLUP_NO_NATIVE=1 ROLLUP_NO_WASM=1 tsc && vite build --config vite.config.ts",
    "build:reminder": "echo '⚠️  REMINDER: Did you increment CACHE_VERSION in service-worker.js?' && npm run build",
    "preview": "vite preview",
    "generate-sitemap": "node scripts/generate-sitemap.mjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:strict": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "save": "git add -A && git commit -m \"chore(save): workspace snapshot\" && git push origin main",
    "setup:affiliates": "node scripts/setup-affiliate-collections.js",
    "setup:booking": "node scripts/setup-booking-collections.js",
    "verify:booking": "node scripts/verify-booking-setup.js",
    "verify:booking-sounds": "node scripts/verify-booking-sounds.mjs",
    "verify:affiliates": "node scripts/verify-affiliate-collections.js",
    "sync:translations": "tsx scripts/syncTranslations.ts",
    "migrate:images": "tsx scripts/migrateImagesToAppwrite.ts",
    "validate": "node scripts/validate-imports.cjs",
    "validate:imports": "node scripts/validate-imports.cjs",
    "clean": "pwsh -NoProfile -Command Remove-Item -Recurse -Force node_modules/.vite,.vite,dist -ErrorAction SilentlyContinue",
    "clean:all": "pwsh -NoProfile -Command Remove-Item -Recurse -Force node_modules/.vite,.vite,.cache,dist,build -ErrorAction SilentlyContinue",
    "clean:vscode": "pwsh -NoProfile -File scripts/cleanup-vscode-emergency.ps1",
    "clean:deep": "pwsh -NoProfile -Command Remove-Item -Recurse -Force node_modules,.vite,.cache,dist,build,coverage -ErrorAction SilentlyContinue",
    "reset:vscode": "npm run clean:vscode",
    "fix:vscode": "pwsh -NoProfile -File scripts/cleanup-vscode-emergency.ps1",
    "check": "npm run lint && npm run type-check && npm run validate",
    "precommit": "npm run check && npm run test",
    "migrate:flags": "tsx scripts/migrateFlagImages.ts",
    "mobile:android": "cd mobile && npx react-native run-android",
    "mobile:ios": "cd mobile && npx react-native run-ios",
    "mobile:start": "cd mobile && npx react-native start",
    "schema:export:hotels": "node scripts/exportHotelsSchema.cjs",
    "schema:validate:hotels": "node scripts/validateHotelsPayloads.cjs",
    "analyze": "vite build --mode analyze",
    "analyze:bundle": "vite-bundle-visualizer",
    "monitor:files": "node scripts/file-size-monitor.mjs",
    "monitor:watch": "nodemon --watch . --ext ts,tsx,js,jsx --ignore node_modules --exec 'npm run monitor:files'",
    "size:check": "npm run monitor:files",
    "size:report": "npm run monitor:files > file-size-report.txt",
    "precheck": "npm run monitor:files",
    "migrate:services": "node scripts/appwrite-migration.cjs report",
    "migrate:extract": "node scripts/appwrite-migration.cjs extract",
    "migrate:all": "node scripts/appwrite-migration.cjs extract-all",
    "refactor:check": "npm run monitor:files && npm run lint",
    "refactor:plan": "echo 'See docs/COMPONENT_REFACTORING_GUIDE.md for guidelines'",
    "refactor:verify": "npm run type-check && npm run test",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "@types/react-calendar": "^3.9.0",
    "@types/react-helmet": "^6.1.11",
    "appwrite": "^21.4.0",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "express": "^5.1.0",
    "framer-motion": "^11.18.2",
    "i18next": "^25.7.4",
    "lucide-react": "^0.562.0",
    "node-appwrite": "^20.3.0",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.1",
    "react-calendar": "^6.0.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.1.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.48.0",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^16.5.3",
    "react-icons": "^5.0.0",
    "react-responsive": "^10.0.1",
    "react-router-dom": "^6.28.0",
    "react-share": "^5.1.0",
    "tailwind-merge": "^2.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@esbuild/linux-arm64": "0.25.11",
    "@esbuild/linux-x64": "0.25.11",
    "@eslint/js": "^9.39.1",
    "@rollup/rollup-linux-x64-gnu": "^4.22.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "dotenv": "^16.4.5",
    "esbuild": "0.25.11",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "glob": "^13.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3",
    "vite": "^6.4.1",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": "22.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "0.25.11",
    "@esbuild/darwin-x64": "0.25.11",
    "@esbuild/win32-x64": "0.25.11",
    "@rollup/rollup-darwin-arm64": "^4.22.0",
    "@rollup/rollup-darwin-x64": "^4.22.0",
    "@rollup/rollup-linux-x64-musl": "^4.22.0",
    "@rollup/rollup-win32-x64-msvc": "^4.22.0"
  },
  "keywords": [
    "massage",
    "booking",
    "react",
    "typescript",
    "modular",
    "mobile",
    "spa"
  ],
  "author": "IndaStreet Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Philip2024394/website-massage-.git"
  }
}

{
  "name": "indastreet-massage-platform",
  "version": "2.0.0",
  "description": "Modular massage booking platform with separate apps for each user type",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cross-env ROLLUP_NO_NATIVE=1 tsc && vite build --config vite.config.deployment.ts",
    "build:local": "cross-env ROLLUP_NO_NATIVE=1 tsc && vite build",
    "build:vercel": "node esbuild.config.cjs",
    "build:vite": "tsc && vite build --config vite.config.simple.ts",
    "build:production": "echo '⚠️  REMINDER: Did you increment CACHE_VERSION in service-worker.js?' && npm run build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 9999",
    "lint:strict": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "save": "git add -A && git commit -m \"chore(save): workspace snapshot\" && git push origin main",
    "sync:translations": "tsx scripts/syncTranslations.ts",
    "migrate:images": "tsx scripts/migrateImagesToAppwrite.ts",
    "migrate:flags": "tsx scripts/migrateFlagImages.ts",
    "mobile:android": "cd mobile && npx react-native run-android",
    "mobile:ios": "cd mobile && npx react-native run-ios",
    "mobile:start": "cd mobile && npx react-native start"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "@types/react-calendar": "^3.9.0",
    "appwrite": "^21.4.0",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.546.0",
    "node-appwrite": "^20.3.0",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.1",
    "react-calendar": "^6.0.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.48.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^5.0.0",
    "react-router-dom": "^6.28.0",
    "tailwind-merge": "^2.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.16",
    "cross-env": "^10.1.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^16.5.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3",
    "vite": "^6.4.1",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.22.0",
    "@rollup/rollup-darwin-x64": "^4.22.0",
    "@rollup/rollup-linux-x64-gnu": "^4.22.0",
    "@rollup/rollup-linux-x64-musl": "^4.22.0",
    "@rollup/rollup-win32-x64-msvc": "^4.22.0"
  },
  "keywords": [
    "massage",
    "booking",
    "react",
    "typescript",
    "modular",
    "mobile",
    "spa"
  ],
  "author": "IndaStreet Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Philip2024394/website-massage-.git"
  }
}
